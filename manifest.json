{
    "manifest_version": 3,
    "name": "Website Data Collector",
    "version": "2.0",
    "description": "Detects what personal information a website collects",
    "permissions": [
      "tabs",
      "cookies",
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.jpg",
        "48": "icons/icon48.jpg",
        "128": "icons/icon128.jpg"
      }
    },
    "icons": {
      "16": "icons/icon16.jpg",
      "48": "icons/icon48.jpg",
      "128": "icons/icon128.jpg"
    }
  }
  